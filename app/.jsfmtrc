{


  "// docs, basically" : "https://github.com/paulirish/sublime-jsfmt#formatting-rules",


  "preset": "jquery",
  "plugins": [
        "esformatter-semicolons",
        "esformatter-braces"
    ],
    "indent": {
        "value": "    "  // four spaces, b.
    },
    "whiteSpace": {
        "before": {
            "ArgumentList": 1,
            "ArgumentListObjectExpression": 1,
            "ArgumentListFunctionExpression": 1,
            "ArgumentListArrayExpression": 1
        },
        "after": {
            "ArgumentList": 1,
            "ArgumentListObjectExpression": 1,
            "ArgumentListFunctionExpression": 1,
            "ArgumentListArrayExpression": 1
        }
    }
}
